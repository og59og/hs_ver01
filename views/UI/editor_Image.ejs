<div class="modal" id="editorImage">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="height: 30px; padding: 5px 15px;">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" style="font-size: 13px; color: black; padding: 0px; font-weight: bold; ">Editor Image</h4>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th style="text-align: center; width: 60px ">#</th>
                            <th style="text-align: center">이름</th>
                            <th style="text-align: center">등록이미지</th>
                            <th style="text-align: center">등록일</th>
                            <th class="value_menu_th_activation">변경</th>
                        </tr>
                        </thead>
                        <tbody id="editorImageTable">

                        </tbody>
                    </table>
                    <div class="pull-right" style="padding-top: 20px;">
                        <button type="button" class="btn btn-primary" id="addeditorimage">추가 </button>
                        <button type="button" class="btn btn-primary"  id="addImageSave" >저장 </button>
                        <button type="button" class="btn btn-danger" data-dismiss="modal" >닫기 </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


